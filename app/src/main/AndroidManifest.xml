<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="de.uni.oldenburg.spotvisitor" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainAction"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".compass.CompassActivity" />
        <activity android:name=".location.LocationActivity" />
        <activity android:name=".detail.DetailActivity" />
        <activity
            android:name=".entry.EntryActivity"
            android:label="@string/title_activity_entry" >
        </activity>
    </application>

</manifest>
